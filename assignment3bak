#create function with picture as a parameter.
def showRoads():
  #make test image
  canvas=makeEmptyPicture(300,300)
  
  #put black dots on test image
  i=0
  j=0
  for x in range(0,300):
    for y in range(0,300):
      pixl=getPixel(canvas,x,y)  
      if i==200: #pixl==getPixel(canvas,40,40):
        j=j+1
        if j==100:
          blck=makeColor(0,0,0)
          setColor(pixl,blck)
          j=0
        i=0
      i=i+1
  repaint(canvas)

  #find black dots
  #create list
  count=0
  for x in range(0,300):
    for y in range(0,300):
      pix=getPixel(canvas,x,y)
      r=getRed(pix)
      g=getGreen(pix)
      b=getBlue(pix)
      if r<10 and g<10 and b<10:
        count=count+1
        
  index=0     
  blackPixelX=[count]
  blackPixelY=[count]
  for x in range(0,300):
    for y in range(0,300):
      pix=getPixel(canvas,x,y)
      r=getRed(pix)
      g=getGreen(pix)
      b=getBlue(pix)
      if r<10 and g<10 and b<10:
        blackPixelX[index]=x
        blackPixelY[index]=y
        print x,y
        index=index+1
  print count
  print blackPixelX,blackPixelY      
  #store each dot in a list
#connect with lihnes in varies ways.
